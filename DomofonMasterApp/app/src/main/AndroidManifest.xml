<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.domofonmasterapp">

    <uses-feature android:name="android.hardware.usb.host" />
    <uses-permission
        android:name="android.permission.INTERNET"
        tools:ignore="ManifestOrder" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.CityActivity"
                  android:label="Города"/>
        <activity android:name=".Activity.IntercomActivity"
            android:label="Домофоны"/>
        <activity
            android:name=".Activity.StreetActivity"
            android:label="Улицы" />
        <activity
            android:name=".Activity.HouseActivity"
            android:label="Дома" />
        <activity
            android:name=".Activity.PorchActivity"
            android:label="Подъезды" />
        <activity
            android:name=".Activity.KeyActivity"
            android:label="Ключи" />
        <activity
            android:name=".Activity.FavouritesActivity"
            android:label="Избранное"/>
        <activity
            android:name=".Activity.DetailKeyActivity"
            android:label=""/>
        <activity
            android:name=".Activity.AddKeyActivity"
            android:label=""/>
        <activity
            android:name=".Activity.WriteKeyActivity"
            android:label=""/>

    </application>

</manifest>